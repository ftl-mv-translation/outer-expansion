#: src-en/data/events_special_aea_shard.xml:85
msgid ""
"data/events_special_aea_shard.xml$//textList[@name=\"TEXT_GUARD_AEA_SHARD_CONTACT\"]/text"
msgstr ""
"A large crystal appears on your screen, you can see the distictive white "
"mark across their shoulder indicating that they're a member of the Shard-"
"Forged."

#: src-en/data/events_special_aea_shard.xml:90
msgid ""
"data/events_special_aea_shard.xml$//event[@name=\"GUARD_AEA_SHARD_INTERACT\"]/choice[1]/text"
msgstr "Request supplies."

#: src-en/data/events_special_aea_shard.xml:94
msgid ""
"data/events_special_aea_shard.xml$//event[@name=\"GUARD_AEA_SHARD_INTERACT\"]/choice[2]/text"
msgstr "Demand supplies."

#: src-en/data/events_special_aea_shard.xml:98
msgid ""
"data/events_special_aea_shard.xml$//event[@name=\"GUARD_AEA_SHARD_INTERACT\"]/choice[@req=\"weapons\"]/text"
msgstr "(Upg. Weapons) Threaten the guard."

#: src-en/data/events_special_aea_shard.xml:102
msgid ""
"data/events_special_aea_shard.xml$//event[@name=\"GUARD_AEA_SHARD_INTERACT\"]/choice[4]/text"
msgstr "Change your mind and leave the guard alone."

#: src-en/data/events_special_aea_shard.xml:104
msgid ""
"data/events_special_aea_shard.xml$//event[@name=\"GUARD_AEA_SHARD_INTERACT\"]/choice[4]/event/text"
msgstr ""
"You apologize and end comms. The guard makes no attempts to communicate "
"further."

#: src-en/data/events_special_aea_shard.xml:106
