msgid ""
"data/events_sector_aea_shard.xml$//event[@name=\"AEA_SHARD_ARGUMENT\"]/choice[1]/event/choice[4]/text"
msgstr "\"I'm not really sure...\""

#: src-en/data/events_sector_aea_shard.xml:268
msgid ""
"data/events_sector_aea_shard.xml$//event[@name=\"AEA_SHARD_ARGUMENT\"]/choice[1]/event/choice[4]/event/text"
msgstr ""
