#: src-en/data/events_special_aea_shleg.xml:368
msgid ""
"data/events_special_aea_shleg.xml$//event[@name=\"NEBULA_TRANSPORT_AEA_SHLEG_LOOT\"]/text"
msgstr ""
"This beacon hosts an Anarchist transport. It's safe to bet the ship is most "
"likely carrying some special tech. If you can kill the crew onboard, the "
"cargo will be yours."

#: src-en/data/events_special_aea_shleg.xml:371
msgid ""
"data/events_special_aea_shleg.xml$//event[@name=\"NEBULA_TRANSPORT_AEA_SHLEG_LOOT\"]/choice[1]/text"
msgstr "Demand the surrender of the goods."

#: src-en/data/events_special_aea_shleg.xml:373
msgid ""
"data/events_special_aea_shleg.xml$//event[@name=\"NEBULA_TRANSPORT_AEA_SHLEG_LOOT\"]/choice[1]/event/text"
msgstr "You prepare to secure the cargo by force."

#: src-en/data/events_special_aea_shleg.xml:376
msgid ""
"data/events_special_aea_shleg.xml$//event[@name=\"NEBULA_TRANSPORT_AEA_SHLEG_LOOT\"]/choice[1]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_special_aea_shleg.xml:382
msgid ""
"data/events_special_aea_shleg.xml$//event[@name=\"NEBULA_TRANSPORT_AEA_SHLEG_LOOT\"]/choice[2]/text"
msgstr "Avoid the ship."

#: src-en/data/events_special_aea_shleg.xml:384
msgid ""
"data/events_special_aea_shleg.xml$//event[@name=\"NEBULA_TRANSPORT_AEA_SHLEG_LOOT\"]/choice[2]/event/text"
msgstr "The ship stays outside your weapons range, and eventually jumps away."

#: src-en/data/events_special_aea_shleg.xml:386
