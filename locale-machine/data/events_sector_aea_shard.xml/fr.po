#: src-en/data/events_sector_aea_shard.xml:351
msgid ""
"data/events_sector_aea_shard.xml$//event[@name=\"AEA_SHARD_DRUNK\"]/choice[5]/event/text"
msgstr ""
"« ET VOILÀ ! Regardez tout le monde, je suis réprimé ! » Le vaisseau Forgé-"
"Éclats riposte !"

#: src-en/data/events_sector_aea_shard.xml:354
msgid ""
"data/events_sector_aea_shard.xml$//event[@name=\"AEA_SHARD_DRUNK\"]/choice[5]/event/choice/text"
